{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Celery Flower",
      "type": "shell",
      "command": "celery",
      "args": [
        "--app=celery_apps.io_worker.main:celery_app",
        "flower",
        "--port=5555"
      ],
      "group": "build",
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "new"
      },
      "runOptions": {
        "runOn": "folderOpen"
      },
      "problemMatcher": []
    },
    {
      "label": "Celery Beat",
      "type": "shell",
      "command": "celery",
      "args": [
        "--app=celery_apps.io_worker.main:celery_app",
        "beat",
        "--loglevel=info"
      ],
      "group": "build",
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "new"
      },
      "runOptions": {
        "runOn": "folderOpen"
      },
      "problemMatcher": []
    }
  ]
}
